{
    "extends": "airbnb-base",
    "parser": "babel-eslint",
    "parserOptions": {
        "ecmaVersion": 8
    },
    "rules": {
        "semi": 0,
        "prefer-const": 0,
        "object-curly-newline": 0,
        "no-var": 0,
        "padded-blocks": 0,
        "object-shorthand": 0,
        "prefer-destructuring": 0,
        "guard-for-in": 0,
        "no-restricted-syntax": 0,
        "vars-on-top": 0,
        "arrow-body-style": 0,
        "class-methods-use-this": 0,
        "prefer-template": 0,
        "no-useless-constructor": 0,
        "func-names": 0,
        "max-len": 0,
        "new-cap": 0,
        "no-useless-escape": 0,
        "global-require": 0
      }
}